<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10" alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.TrackDetails">
    <Label text="Track Details"/>
    <Label text="Track Name:"/>
    <TextField fx:id="trackName" editable="false"/>
    <Label text="Artist:"/>
    <TextField fx:id="trackArtist" editable="false"/>
    <Label text="Genre:"/>
    <TextField fx:id="trackGenre" editable="false"/>
    <Button fx:id="playPreviewButton" text="Play Preview" onAction="#playPreview"/>
</VBox>
